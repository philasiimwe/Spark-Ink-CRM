# Supabase Configuration
VITE_SUPABASE_URL=your-project-url.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key

# Gemini AI
VITE_GEMINI_API_KEY=your-gemini-api-key

# Google OAuth (for Calendar/Meet)
VITE_GOOGLE_CLIENT_ID=your-google-client-id
VITE_GOOGLE_CLIENT_SECRET=your-google-client-secret

# Email Integration (Gmail API)
VITE_GMAIL_CLIENT_ID=your-gmail-client-id
VITE_GMAIL_CLIENT_SECRET=your-gmail-client-secret

# Twilio (for SMS/Voice)
VITE_TWILIO_ACCOUNT_SID=your-twilio-sid
VITE_TWILIO_AUTH_TOKEN=your-twilio-token
VITE_TWILIO_PHONE_NUMBER=your-twilio-number

# Stripe (for payments if needed)
VITE_STRIPE_PUBLIC_KEY=your-stripe-public-key

# App Configuration
VITE_APP_URL=http://localhost:5173
VITE_API_URL=http://localhost:3000

# Feature Flags
VITE_ENABLE_AI_FEATURES=true
VITE_ENABLE_EMAIL_SYNC=true
VITE_ENABLE_TELEPHONY=true
VITE_ENABLE_WEBHOOKS=true
